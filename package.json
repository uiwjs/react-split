{
  "name": "@uiw/react-split",
  "version": "5.6.6",
  "description": "A piece of view can be divided into areas where the width or height can be adjusted by dragging.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "homepage": "https://uiwjs.github.io/react-split",
  "scripts": {
    "prepare": "npm run build",
    "released": "npm run build && npm run doc",
    "doc": "ENTRYDIR=website kkt build",
    "start": "ENTRYDIR=website kkt start",
    "build": "npm run ts:build && npm run types:esm && npm run types:cjs && npm run css:build",
    "types:build": "tsbb types --sourceRoot src --target ESNEXT",
    "watch": "npm run ts:watch & npm run types:watch & npm run css:watch",
    "types:watch": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "npm run types:build -- --outDir ../lib/esm",
    "types:cjs": "npm run types:build -- --outDir ../lib/cjs",
    "css:build": "compile-less -d src -o lib/esm",
    "css:watch": "compile-less -d src -o lib/esm --watch",
    "ts:watch": "tsbb watch --env-name esm:dev --env-name cjs --target react",
    "ts:build": "tsbb build --target react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-split.git"
  },
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "files": [
    "src",
    "lib"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@kkt/loader-less": "5.10.3",
    "@kkt/loader-raw": "5.10.3",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@uiw/react-code-preview": "1.11.8",
    "@uiw/react-github-corners": "1.2.0",
    "@uiw/react-markdown-preview": "1.0.9",
    "@uiw/reset.css": "1.0.4",
    "compile-less-cli": "1.6.0",
    "kkt": "5.10.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tsbb": "1.7.7",
    "uiw": "4.6.14"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
